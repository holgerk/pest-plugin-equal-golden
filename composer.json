{
    "name": "holgerk/pest-plugin-equal-golden",
    "description": "Similar to the `toEqual` assertion, but with automatic expectation generation.",
    "keywords": [
        "php",
        "framework",
        "pest",
        "unit",
        "test",
        "testing",
        "plugin"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "nikic/php-parser": "^5.0",
        "pestphp/pest": "^2.33",
        "pestphp/pest-plugin": "^2.0.1",
        "symfony/var-exporter": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Holgerk\\EqualGolden\\": "src/"
        },
        "files": [
            "src/Autoload.php"
        ]
    },
    "require-dev": {
        "pestphp/pest-dev-tools": "^2.9"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "pest": {
            "plugins": [
                "Holgerk\\EqualGolden\\Plugin"
            ]
        }
    },
    "scripts": {
        "refacto": "rector",
        "lint": "pint",
        "test:refacto": "rector --dry-run",
        "test:lint": "pint --test",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:refacto",
            "@test:lint",
            "@test:unit"
        ]
    }
}
